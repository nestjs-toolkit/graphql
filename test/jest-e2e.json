{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@nestjs-toolkit/graphql/(.*)": "<rootDir>/../libs/graphql/src/$1",
    "@nestjs-toolkit/graphql": "<rootDir>/../libs/graphql/src"
  }
}
