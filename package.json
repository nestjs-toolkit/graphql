{
  "name": "@nestjs-toolkit/graphql",
  "version": "0.1.3",
  "description": "core helpers",
  "author": "Valmir Barbosa",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "postbuild": "cp package.json dist/libs/graphql",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc": "doctoc docs --title='## Summary'",
    "commit": "git-cz",
    "release": "release-it --patch",
    "lib:build": "yarn prebuild && nest build graphql --tsc && yarn postbuild"
  },
  "peerDependencies": {
    "@nestjs-toolkit/base": "*",
    "moment-timezone": "^0.5.34"
  },
  "dependencies": {
    "@nestjs/graphql": "^10.0.15",
    "@nestjs/apollo": "^10.0.14",
    "graphql": "^16.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/common": "^8.4.6",
    "@nestjs/core": "^8.4.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.6",
    "@types/jest": "28.1.1",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "apollo-server-fastify": "^3.8.2",
    "bson": "^4.6.4",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql-redis-subscriptions": "^2.4.2",
    "graphql-subscriptions": "^2.0.0",
    "ioredis": "^5.0.6",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@nestjs-toolkit/graphql(|/.*)$": "<rootDir>/libs/graphql/src/$1"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "packageManager": "yarn@3.2.1"
}
